		<js minimize="false">/js-vendor/angular-ui-router.min.js</js>
		<js minimize="false">/js-vendor/angular-translate.min.js</js>
		<js minimize="false">/js-vendor/angular-translate-storage-local.js</js>
		<js minimize="false">/js-vendor/angular-translate-storage-cookie.js</js>
		<js>/js-vendor/angular-translate-loader-partial.js</js>
		<js minimize="false">/js-vendor/SecuritySrvProvider.js</js>
		<js minimize="false">/js-vendor/moment-with-langs.min.js</js>
		<js minimize="false">/js-vendor/QFormValidationProvider.js</js>
